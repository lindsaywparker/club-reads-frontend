{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
      "react/jsx-filename-extension": [0],
      "max-len": [0],
      "no-shadow": [0]
    },
    "globals": {
      "document": true,
      "fetch": true
    },
    "env": {
      "jest": true
    }
}
